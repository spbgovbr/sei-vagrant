ENVIRONMENT_VERSION=9.0

DATABASE_IMAGE=guilhermeadc/sei4_mysql
DATABASE_TYPE=MySql
DATABASE_HOST=mysql
DATABASE_PORT=3306
DATABASE_DATA_DIR=/var/lib/mysql

SEI_DATABASE_NAME=sei
